pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
             git branch: 'master',url: 'https://github.com/sarikabharath/repo1.git' 
                sh 'mvn compile'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}

